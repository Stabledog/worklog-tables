{
	"manifest_version": 2,

	"name": "Worklogian",
	"description": "My work log tools",

	"content_scripts": [
		{
			"matches": ["https://sites.google.com/site/stabledogpersonal/journal/chromeextensiontoscrapeworklogs/worklog-tables-test"],
			"js": [ "mycontent.js" ]
		}
	],

	"version": "1.0",

	"permissions":[
		"https://sites.google.com/*"
		],

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	}
}
