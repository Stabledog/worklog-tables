{
	"manifest_version": 2,

	"name": "Worklogian",
	"description": "Sample from http://developer.chrome.com/extensions/getstarted.html",

	"content_scripts": [
		{
			"matches": ["https://sites.google.com/site/stabledogpersonal/journal/chromeextensiontoscrapeworklogs/worklog-tables-test"],
			"js": [ "mycontent.js" ]
		}
	]

	"version": "1.0",

	"permissions":[
		"https://secure.flickr.com/"
		],

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	}
}
